{
	"name": "trade-ace",
	"version": "0.0.1",
	"description": "Trade ACE",
	"scripts": {
		"dev": "npm run development",
		"development": "mix",
		"watch": "mix watch",
		"watch-poll": "mix watch -- --watch-options-poll=1000",
		"hot": "mix watch --hot",
		"prod": "npm run production",
		"production": "mix --production",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style 'src/assets/scss/**/*.scss'",
		"lint:js": "wp-scripts lint-js 'src/assets/js/**/*.js'",
		"makepot": "wp i18n make-pot --domain='trade-ace' --include='assets/js,*.php' . --headers='{\"Report-Msgid-Bugs-To\":\"https://shopeo.cn/contact/\"}' --file-comment=\"Copyright (c) $(date +'%Y') SHOPEO. All Rights Reserved.\" languages/trade-ace.pot",
		"packages-update": "wp-scripts packages-update"
	},
	"author": "SHOPEO",
	"homepage": "https://shopeo.cn",
	"license": "MIT",
	"devDependencies": {
		"@wordpress/scripts": "^30.3.0",
		"autoprefixer": "^10.4.20",
		"laravel-mix": "^6.0.49",
		"postcss": "^8.4.47",
		"postcss-import": "^16.1.0",
		"resolve-url-loader": "^5.0.0",
		"rtlcss": "^4.3.0",
		"sass": "^1.80.5",
		"sass-loader": "^16.0.2",
		"tailwindcss": "^3.4.14",
		"wp-readme-to-markdown": "^1.0.1"
	}
}
